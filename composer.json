{
    "name": "webshr/wp-htmx-router",
    "description": "A custom PHP router for the WP HTMX plugin, based on the Rareloop router package",
    "keywords": [
        "WordPress",
        "router",
        "HTMX"
    ],
    "homepage": "https://github.com/webshr/wp-htmx-router",
    "license": "MIT",
    "authors": [
        {
            "name": "Henrik Liebel",
            "email": "mail@henrikliebel.com",
            "homepage": "https://webshore.io",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3||^8.0",
        "altorouter/altorouter": "^2.0",
        "laminas/laminas-diactoros": "^3.3",
        "mindplay/middleman": "^4.0",
        "php-di/php-di": "^7.0",
        "psr/container": "^2.0",
        "psr/http-message": "^2.0",
        "psr/http-server-middleware": "^1.0",
        "spatie/macroable": "^2.0"
    },
    "require-dev": {
        "10up/wp_mock": "^0.4.2",
        "mockery/mockery": "^1.6.11",
        "php-coveralls/php-coveralls": "^2.7",
        "phpunit/phpunit": "^10.5",
        "squizlabs/php_codesniffer": "^3.9.1"
    },
    "autoload": {
        "psr-4": {
            "Webshr\\WPHTMX\\Router\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Webshr\\WPHTMX\\Router\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
